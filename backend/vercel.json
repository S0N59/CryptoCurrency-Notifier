{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/health",
            "destination": "/api/health.js"
        },
        {
            "source": "/api/telegram/webhook",
            "destination": "/api/telegram/webhook.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ]
}
